--{Prison Life}--
if game.PlaceId == 155615604 then
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/cupixclient/source/main/prisonlife'),true))()
	else
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/cupixclient/source/main/universal'),true))()
end
